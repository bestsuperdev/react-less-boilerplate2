{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "a react boilerplate",
  "main": "main.js",
  "scripts": {
    "start": "node ./server.js",
    "mock": "cd ./mock && json-server --watch db.json --routes routes.json --port 9001",
    "lint": "eslint ./src",
    "test": "karma start  --config karma.conf.js",
    "clean": "rimraf dist",
    "deploy": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "dependencies": {
    "beyond-lib": "^1.0.2",
    "beyond-remote": "^0.4.5",
    "normalize.css": "^5.0.0",
    "es6-promise": "^4.0.5",
