{
  "name": "react-less-boilerplate2",
  "version": "2.0.0",
  "description": "a react boilerplate",
  "main": "main.js",
  "scripts": {
    "start": "node ./server.js",
    "mock": "cd ./mock && json-server --watch db.json --routes routes.json --port 9001",
    "lint": "eslint ./src",
    "test": "karma start  --config karma.conf.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack.pro.config.js --optimize-minimize",
    "build:mvc": "cross-env NODE_ENV=production webpack --config webpack.mvc.config.js --optimize-minimize"
  },
  "dependencies": {
    "@types/webpack-env": "^1.13.2",
    "beyond-lib": "^1.3.3",
    "beyond-remote": "^0.5.0",
    "classnames": "^2.2.5",
    "es6-promise": "^4.0.5",
    "normalize.css": "^7.0.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.2",
    "express-http-proxy": "^1.0.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.5.2",
    "json-server": "^0.12.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^2.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "less-plugin-autoprefix": "^1.5.1",
    "opn": "^5.1.0",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.0",
    "webpack": "^3.8.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestsuperdev/react-less-boilerplate2.git"
  },
  "author": "Peng mingze <mingzepeng@gmail.com>",
